{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#397367",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .FullName }} ",
          "type": "session"
        },
        {
          "background": "#63CCCA",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "folder",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#5DA399",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#42858C",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718{{ if .DockerfileExists }} \u26A1 {{ .DockerVersion }}{{ end }} ",
          "type": "docker"
        },
        {
          "background": "#42858C",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718{{ if .TerraformInitialized }} \u26A1 {{ .TerraformVersion }}{{ if .TerraformWorkspace }} / {{ .TerraformWorkspace }}{{ end }}{{ end }} ",
          "type": "terraform"
        },
        {
          "background": "#42858C",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_namespace": true
          },
          "style": "diamond",
          "template": " \u2388 {{ .Context }}{{ if ne .Namespace \"default\" }}/{{ .Namespace }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "kubernetes"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
