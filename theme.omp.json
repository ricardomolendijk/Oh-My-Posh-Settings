{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.Folder}}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#5BCCF0",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .FullName }}",
          "type": "session"
        },
        {
          "background": "#5cb2ec",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#7396de",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#8d77c2",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "{{ if .IsTerraformInitialized }}\uDB84\uDC62 {{ .Version }}{{ if .WorkspaceName }}/{{ .WorkspaceName }}{{ end }}{{ end }} ",
          "type": "terraform"
        },
        {
          "background": "#41B8FF",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "{{ if .DockerfileExists }}\uF308 {{ .DockerVersion }}{{ end }} ",
          "type": "docker"
        },
        {
          "background": "#3D68FF",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " ï´± {{.Context}}{{if .Namespace}}/{{.Namespace}}{{end}}",
          "type": "kubectl",
          "parse_kubeconfig": true
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
